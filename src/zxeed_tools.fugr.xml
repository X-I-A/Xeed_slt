  <asx:values>
   <AREAT>Xeed SLT to HTTP Plugin Tools</AREAT>
   <INCLUDES>
    <SOBJ_NAME>LZXEED_TOOLSTOP</SOBJ_NAME>
    <SOBJ_NAME>SAPLZXEED_TOOLS</SOBJ_NAME>
   </INCLUDES>
   <FUNCTIONS>
    <item>
     <FUNCNAME>Z_XEED_ARCHIVE_DATA</FUNCNAME>
     <REMOTE_CALL>R</REMOTE_CALL>
     <SHORT_TEXT>Save Data to a local path</SHORT_TEXT>
     <IMPORT>
      <RSIMP>
       <PARAMETER>I_CONTENT</PARAMETER>
       <TYP>STRING</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>I_PATH</PARAMETER>
       <TYP>ZXEED_D_PATHINTERN</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>I_SEQ_NO</PARAMETER>
       <TYP>CHAR20</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>I_MT_ID</PARAMETER>
       <TYP>DMC_MT_IDENTIFIER</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>I_TABNAME</PARAMETER>
       <TYP>TABNAME</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>I_AGE</PARAMETER>
       <TYP>NUMC10</TYP>
      </RSIMP>
     </IMPORT>
     <EXCEPTION>
      <RSEXC>
       <EXCEPTION>PATH_ERROR</EXCEPTION>
      </RSEXC>
     </EXCEPTION>
     <DOCUMENTATION>
      <RSFDO>
       <PARAMETER>I_CONTENT</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Data</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>I_PATH</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Archive File Location</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>I_SEQ_NO</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Sequence Number</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>I_MT_ID</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Mass Transfer ID</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>I_TABNAME</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Table Name</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>I_AGE</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Age Number</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>PATH_ERROR</PARAMETER>
       <KIND>X</KIND>
       <STEXT>Cannot identifer the correction Path</STEXT>
      </RSFDO>
     </DOCUMENTATION>
    </item>
    <item>
     <FUNCNAME>Z_XEED_POST_DATA</FUNCNAME>
     <REMOTE_CALL>R</REMOTE_CALL>
     <SHORT_TEXT>Post SLT Data To Configured Destination</SHORT_TEXT>
     <IMPORT>
      <RSIMP>
       <PARAMETER>I_DESTINATION</PARAMETER>
       <TYP>RFCDEST</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>I_CONTENT</PARAMETER>
       <TYP>STRING</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>I_SEQ_NO</PARAMETER>
       <TYP>CHAR20</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>I_MT_ID</PARAMETER>
       <TYP>DMC_MT_IDENTIFIER</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>I_TABNAME</PARAMETER>
       <TYP>TABNAME</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>I_AGE</PARAMETER>
       <TYP>NUMC10</TYP>
      </RSIMP>
     </IMPORT>
     <EXPORT>
      <RSEXP>
       <PARAMETER>E_STATUS</PARAMETER>
       <TYP>I</TYP>
      </RSEXP>
      <RSEXP>
       <PARAMETER>E_RESPONSE</PARAMETER>
       <TYP>STRING</TYP>
      </RSEXP>
     </EXPORT>
     <EXCEPTION>
      <RSEXC>
       <EXCEPTION>RFC_ERROR</EXCEPTION>
      </RSEXC>
      <RSEXC>
       <EXCEPTION>CONNECTION_ERROR</EXCEPTION>
      </RSEXC>
     </EXCEPTION>
     <DOCUMENTATION>
      <RSFDO>
       <PARAMETER>I_DESTINATION</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Logical destination (specified in function call)</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>I_CONTENT</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Plein Text Content String</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>I_SEQ_NO</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Sequence Number</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>I_MT_ID</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Mass Transfer ID</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>I_TABNAME</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Table Name</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>I_AGE</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Age Number</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>E_STATUS</PARAMETER>
       <KIND>P</KIND>
       <STEXT>HTTP Return Code</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>E_RESPONSE</PARAMETER>
       <KIND>P</KIND>
       <STEXT>HTTP Response Body</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>RFC_ERROR</PARAMETER>
       <KIND>X</KIND>
       <STEXT>Cannot create http client from RFC</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>CONNECTION_ERROR</PARAMETER>
       <KIND>X</KIND>
       <STEXT>Cannot Communicate to Destination</STEXT>
      </RSFDO>
     </DOCUMENTATION>
    </item>
